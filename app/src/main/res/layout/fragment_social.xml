<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.social"
    android:background="@drawable/bg1">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/shojumaru"
        android:text="INTENT"
        android:textColor="@color/blue1"
        android:textSize="30dp" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="300dp">
        <ImageView
            android:id="@+id/IG"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/logo_instagram"
            android:layout_gravity="center_horizontal"
            />
        <ImageView
            android:id="@+id/YT"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/logo_youtube"
            android:layout_marginLeft="190dp"
            android:layout_gravity="center_horizontal"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="490dp">
        <ImageView
            android:id="@+id/MAPS"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/logo_map"
            android:layout_gravity="center_horizontal"
            />
        <ImageView
            android:id="@+id/CHROME"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/logo_chrome"
            android:layout_marginLeft="190dp"
            android:layout_gravity="center_horizontal"
            />
    </RelativeLayout>

</FrameLayout>